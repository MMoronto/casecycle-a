<%= form_for(@cases) do |f| %>
  <div class="form-group">
    <%= f.label :casenum %><br>
    <%= f.number_field :casenum, class: "form-control" %></div>

  <div class="form-group">
    <%= f.label :name %><br>
    <%= f.text_field :name, class: "form-control" %></div>

  <div class="form-group">
    <%= f.label :insurance %><br>
    <%= f.text_field :insurance, class: "form-control" %></div>

  <div class="form-group">
    <%= f.label :treatmentplan %><br>
    <%= f.text_field :treatmentplan, class: "form-control" %></div>

  <div class="actions">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>
